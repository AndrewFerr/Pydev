      </command>
<!-- command: Format code-->
      <command
            name="Python Format Code"
            description="Format Code"
            categoryId="org.python.pydev.ui.category.source"
            id="org.python.pydev.editor.actions.pyFormatStd">
      </command>
      <!-- This is for making a keybinding for it-->
      <!-- Extract Method -->
      <command
            name="Python Extract Method"
            description="Extract Method"
            categoryId="org.python.pydev.ui.category.refactoring"
            id="org.python.pydev.refactoring.extractmethod"/>
      <!-- Extract Local Variable -->
      <command
            name="Python Extract Local Variable"
            description="Extract Local Variable"
